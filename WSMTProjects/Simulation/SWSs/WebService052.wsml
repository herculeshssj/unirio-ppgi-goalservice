wsmlVariant _"http://www.wsmo.org/wsml/wsml-syntax/wsml-dl"

namespace { _"http://127.0.0.1/services/EMailValidator.owl#", 
     dc _"http://purl.org/dc/elements/1.1/", 
     wsml _"http://www.wsmo.org/wsml/wsml-syntax#", 
     xsd _"http://www.w3.org/2001/XMLSchema#",
     discovery _"http://wiki.wsmx.org/index.php?title=DiscoveryOntology#" }

webService _"http://127.0.0.1/services/EMailValidator.owl"
	nfp
		dc#title hasValue "Web Service 052"
		dc#description hasValue "Web Service used only to simulation"
	endnfp

importsOntology
            _"http://127.0.0.1/ontology/Concepts.owl"

capability _"http://EMailValidatorCapability"
     nonFunctionalProperties
     	  discovery#discoveryStrategy hasValue discovery#LightweightDLDiscovery
          discovery#discoveryStrategy hasValue discovery#NoPreFilter
     endNonFunctionalProperties

sharedVariables { ?x, ?y }

postcondition
		definedBy
			?x memberOf _"http://127.0.0.1/ontology/Concepts.owl#true-false"
			and
			?y memberOf _"http://127.0.0.1/ontology/Concepts.owl#EMailAddress".
			
			